  },
  "homepage": "https://github.com/alejandronanez/es6-project-boilerplate#readme",
  "devDependencies": {
    "babel-core": "~6.7.7",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.2.4",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.6.0",
    "chai": "~3.5.0",
    "commitizen": "~2.8.1",
    "cz-conventional-changelog": "~1.1.6",
    "eslint": "~2.9.0",
    "eslint-config-airbnb": "~8.0.0",
    "eslint-plugin-import": "~1.6.1",
